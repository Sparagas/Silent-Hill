                          write=(this = Atof(value) * 4096.0)>;

typedef uint32 q20_12     <read=Str("%g", this / 4096.0),
                          write=(this = Atof(value) * 4096.0)>;

typedef int16 q3_12       <read=Str("%g", this / 4096.0),
                          write=(this = Atof(value) * 4096.0)>;
                      
typedef int16 q3_12_deg   <read=Str("%g deg", this / 4096.0 * 360),
                          write=(this = Atof(value) * 4096.0 / 360.0)>;

