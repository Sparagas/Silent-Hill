    Buttons map_18;
    Buttons option_1A;
};

struct ShSaveUserConfig
{
    ControllerConfig controllerConfig_0;
    int8             screenPosX_1C;             /** Range: [-11, 11], default: 0. */
    int8             screenPosY_1D;             /** Range: [-8, 8], default: 0. */
    uint8            optSoundType_1E;           /** Stereo: 0, Monaural: 1, default: Stereo. */
    uint8            optVolumeBgm_1F;           /** Range: [0, 128] with steps of 8, default: 16. */
    uint8            optVolumeSe_20;            /** Range: [0, 128] with steps of 8, default: 16. */
    uint8            optVibrationEnabled_21;    /** Off: 0, On: 128, default: On. */
    uint8            optBrightness_22;          /** Range: [0, 7], default: 3. */
    uint8            optExtraWeaponCtrl_23;     /** Switch: 0, Press: 1, default: Press. */
    uint8            optExtraBloodColor_24;     /** Normal: 0, Green: 2, Violet: 5, Black: 13, default: Normal */
    uint8            optAutoLoad_25;            /** Off: 0, On: 1, default: Off. */
    ubyte            unk_26;
    uint8            optExtraOptionsEnabled_27;
    int8             optExtraViewCtrl_28;       /** Normal: 0, Reverse: 1, default: Normal. */
    int8             optExtraViewMode_29;       /** Normal: 0, Self View: 1, default: Normal. */
    int8             optExtraRetreatTurn_2A;    /** Normal: 0, Reverse: 1, default: Normal. */
    int8             optExtraWalkRunCtrl_2B;    /** Normal: 0, Reverse: 1, default: Normal. */
    int8             optExtraAutoAiming_2C;     /** On: 0, Off: 1, default: On. */
    int8             optExtraBulletAdjust_2D;   /** x1-x6: Range [0, 5], default: x1. */
    uint16           seenGameOverTips_2E;       /** Bitfield tracking seen game-over tips. Each bit corresponds to a tip index (0–14), set bits indicate seen tips. Resets after picking all 15. */
    ubyte            unk_30[4];
    uint32           palLanguageId_34;
};

struct ShSaveGameFooter
{
    uint8  checksumXor[2];
    uint16 magic           <format=hex>;
    /*
    Assert(checksumXor[0] == checksumXor[1]
        && checksumXor[1] == sHchecksumXorCheck(value));
    Assert(magic[0] == magic[1]
        && magic == 0xDCDC);
    */
};

struct ShSaveUserConfigContainer
{
    ShSaveUserConfig shSaveUserConfig <open=true>;
    ubyte            padding[68];
    ShSaveGameFooter footer;  // (124)
};

